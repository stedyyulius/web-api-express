<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>User List</title>
  </head>
  <body>
    <% users.forEach(user=>{ %>
      <table>
        <thead>
          <tr>
            <td>ID</td>
            <td>Username</td>
            <td>Email</td>
            <td>Role</td>
          </tr>        
        </thead>
        <tbody>
          <tr>
            <td><%- user._id %></td>
            <td><%- user.username %></td>
            <td><%- user.email %></td>
            <td><%- user.role %></td>
          </tr>      
        </tbody>
      </table>
  <%  }) %>
  </body>
</html>